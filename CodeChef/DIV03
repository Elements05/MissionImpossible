#include <iostream>
using namespace std;

int main() {
    int T,d[10],k;
    cin>>T;
    while(T-- >0)
    {
        for(int i=0; i<10; i++)
        {
            cin>>d[i];
        }
        cin>>k;
        int s=0;
        for(int i=9;i>=0; i--)
        {
            s+=d[i];
            if(s>k)
            {
                cout<<i+1<<"\n";
                break;
            }
        }
   }
return 0;
}
