#include <iostream>
using namespace std;

int main() {
    int n,k,t;
    int a[n];
    cin>>t;
    while(t-- >0)
    {
        cout<<"\n";
        cin>>n>>k;
        int c=0;
        for(int i=0; i<n; i++)
        {
            cin>>a[i];
            if(k>=a[i])
            {
                k=k-a[i];
                c=1;
                cout<<c;
            }
            else
            {
                c=0;
                cout<<c;
            }
            
        }
    }

	return 0;
}


