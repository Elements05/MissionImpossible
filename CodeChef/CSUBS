#include <iostream>
using namespace std;

int main() {
	int T,N,k,i;
	int a[N];
	cin>>T;
	while(T-- >0)
	{
	    cin>>N>>k;
	    int c=0;
	    for(i=0; i<N; i++)
	    {
	        cin>>a[i];
	        if(a[i]!=k)
	        {
	            c=1;
	        }
	        else
	        {
	            c=0;
	        }
	    }
	    cout<<c<<"\n";
	}
	
	return 0;
}

