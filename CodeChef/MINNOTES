#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,n;
	cin>>t;
	while(t-- >0)
	{
	    cin>>n;
	    long long int a[n],s=0, b=0,k=0;
	    for(int i=0; i<n; i++)
	    {
	        cin>>a[i];
	    }
	    long long int c=a[0];
	    for(int i=1; i<n; i++)
	    {
	       c=gcd(a[i],c); 
	    }
	   
	    for(int i=0; i<n; i++)
	    {
	        if(b<a[i])
	        {
	            b=a[i];
	            k=i;
	        }
	    }
	    a[k]=c;
	     for(int i=0; i<n; i++)
	    {
	        s=s+a[i];
	    }
	    cout<<s/c<<"\n";
	}
	return 0;
}
