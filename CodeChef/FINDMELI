#include <iostream>
using namespace std;

int main() {
    int N,K,a[N];
    int c=0;
    cin>>N>>K;
    for(int i=0;i<N; i++)
    {
        cin>>a[i];
        if(a[i]==K)
        {
            c=1;
            break;
        }
        else
        {
            c=0;
        }
    }
    if(c==1)
    {
        cout<<1<<"\n";
    }
    else
    {
        cout<<-1<<"\n";
    }
    

	return 0;
}
